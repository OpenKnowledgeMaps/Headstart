{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "lib": ["es2022", "DOM"],
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["vis/*"],
      "@js/*": ["vis/js/*"],
      "markjs": ["mark.js/dist/jquery.mark.js"],
      "hypher": ["hypher/dist/jquery.hypher.js"]
    }
  },
  "include": ["vis/**/*"]
}
