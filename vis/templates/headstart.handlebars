<div class="container-headstart">
    <div id="map-loading-screen" class="loading-screen">
        <div id="loading-text" class="loading-text"></div>
        <div id="loading-spinner" class="loading-spinner">
            <span id="spinner-map-icon" class="glyphicon glyphicon-refresh glyphicon-refresh-animate"></span>
        </div>
    </div>
    <div class="vis-col">
        {{#if is_authorview}}
            <div id="title_image" class="titleimage">
                <a id="author_image_link" target="_blank">
                    <div id="author_image" class="authorimage"></div>
                </a>
            </div>
        {{/if}}

        <div id="subdiscipline_title">

            {{#if modern_frontend_enabled}}
            <div id="heading_container"></div>
            {{else}}
            <h4></h4>
            {{/if}}

            {{#if modern_frontend_enabled}}
            <div id="backlink_container"></div>
            {{/if}}

            <p id="context">
              {{#if is_authorview}}
                    <span id="author_living_dates"></span>
                    <span id="author_bio"><a id="author_bio_link" target="_blank"></a></span>
                {{/if}}
                <span id="num_articles"></span>
                <span id="source"></span>
                <span id="timespan"></span>
                <span id="document_types"></span>
                <span id="context-paper_count"></span>
                <span id="context-dataset_count"></span>
                <span id="context-funder"></span>
                <span id="context-project_runtime"></span>
                <span id="search_lang"></span>
                <span id="timestamp"></span>
            </p>

        </div>
<div id="modals"></div>
        <div id="headstart-chart">
            <svg id="chart-svg"></svg>
        </div>
    </div>
    <div class="list-col">
        <div id="list_explorer">
        </div>
    </div>
</div>

{{#if credit_embed}}
    <div id="credit_embed" style="position:absolute; left:0px; bottom:0px; font-size:10px; background-color:#eff3f4; padding:0px 0px 0px 5px; border-radius: 0px 50px 50px 0px; -webkit-border-radius: 0px 50px 50px 0px; -moz-border-radius: 0px 50px 50px 0px;">
<a href="{{ canonical_url }}" target="_blank">created by <b>Open Knowledge Maps </b>
<img id="credit_logo" style="height:30px; padding:0px">
</a></div>
{{/if}}