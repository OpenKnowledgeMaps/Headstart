<!-- Modal -->
<div id="iframe_modal" class="modal fade" role="dialog" tabindex='-1'>
    <div class="modal-dialog modal-lg" id="pdf-modal" draggable="true">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="btn btn-default pull-right" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body text-center">
                <span id="spinner-iframe" class="glyphicon glyphicon-refresh glyphicon-refresh-animate"></span>
                <iframe id="pdf_iframe" class="block" src="" frameborder="0"></iframe>
                <div id="status"></div>
            </div>
        </div>
    </div>
</div>
